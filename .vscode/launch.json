{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test - file",
      "program": "${workspaceRoot}/node_modules/ava/profile.js",
      "args": [
        "${file}"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "name": "cli",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/src/packages/counsel/src/bin.js",
      "stopOnEntry": false,
      "args": [
        "apply",
        "--log-level=warning",
        "--verbose"
      ],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "externalTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "name": "Test",
      "type": "node",
      "request": "launch",
